{
  "name": "myanmar-address",
  "version": "1.0.0",
  "description": "Myanmar state, region, district and township data",
  "keywords": [
    "Myanmar address",
    "Myanmar State and Regions"
  ],
  "main": "index.js",
  "scripts": {
    "format": "standard --fix",
    "test": "standard && jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SpencerAung/myanmar-address.git"
  },
  "author": "Spencer Aung",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SpencerAung/myanmar-address/issues"
  },
  "homepage": "https://github.com/SpencerAung/myanmar-address#readme",
  "devDependencies": {
    "babel-eslint": "^10.0.2",
    "eslint": "^6.1.0",
    "husky": "^3.0.3",
    "jest": "^24.8.0",
    "lint-staged": "^9.2.1",
    "standard": "^13.1.0"
  },
  "lint-staged": {
    "*.js": [
      "npm run format",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "build"
    ],
    "globals": ["describe", "expect", "it"]
  }
}
